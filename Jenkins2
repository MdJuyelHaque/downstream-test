pipeline {
    agent any
    parameters { string(name: 'ENV', defaultValue: 'False', description: '') }
    stages {
        stage("PARAM 1") {
            when {
              branch 'dev'
      }
            steps {
                build job: 'multibrnch2/main',wait: false, parameters: [string(name: 'ENV', value: "${params.ENV}")]
            }
        }
    }
}
