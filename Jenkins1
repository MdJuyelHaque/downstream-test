pipeline {
    agent any
    parameters {
        string(name: 'ENV', description: 'enter env value')
        string(name: 'ENV1', description: 'enter env value')
    }
    stages {
                   
        stage('Example') {
            steps {
                script{
                    echo "${params.ENV}"
                    echo "${params.ENV1}"
                }
            }
        }
    }
}
